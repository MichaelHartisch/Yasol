FLAGS = $(YASOL_COMPILER_OPTION) -DLINUX -std=c++11 -static -DNO_MPI $(YASOL_USED_SOLVER) $(YASOL_USE_CGL)

.cpp.o:
	$(YASOL_CC) -c -I./src -I../Solver/include $(YASOL_PATHS_CGL) $(YASOL_PATHS_SOLVER) -I../Yasol/src  $(FLAGS) -Wno-c++11-compat-deprecated-writable-strings -Wno-deprecated-register -Wno-writable-strings $<

OBJS= src/QBPSolver.o src/QBPSolver_tl.o src/QBPSolver_ctrl.o src/QBPSolver_ml.o src/cliques.o src/commprint.o src/resizer.o src/QBPSolver_All.o

all: QBPS 

QBPS:$(OBJS)
	$(YASOL_AR) -rcs QBPSolver.a *.o

#	 /usr/bin/clang++ -O -o QBPSolver.a $(OBJS) -L/opt/mpich/gcc296/lib
